        <Link>Runtime\CapacityStrategy\CapacityStrategy.cs</Link>
      </Compile>
      <Compile Include="..\E314.DataTypes.Upm\Packages\com.e314.datatypes\Runtime\CapacityStrategy\ICapacityStrategy.cs">
        <Link>Runtime\CapacityStrategy\ICapacityStrategy.cs</Link>
      </Compile>
      <Compile Include="..\E314.DataTypes.Upm\Packages\com.e314.datatypes\Runtime\FastList.cs">
        <Link>Runtime\FastList.cs</Link>
      </Compile>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="E314.Exceptions" Version="1.0.0" />
      <PackageReference Include="E314.Protect" Version="2.0.0" />
    </ItemGroup>

</Project>
